textures/common/dirtymirror
{
	//dirt pass
	qer_editorimage textures/common/dirtymirror.tga
	surfaceparm nolightmap
	portal
	{
		map textures/common/mirror1.tga
		blendFunc GL_ONE GL_ONE
 		depthWrite
	}
	{
		map textures/common/dirtymirror.tga
		blendFunc GL_ONE_MINUS_SRC_ALPHA GL_SRC_ALPHA
	}
}

textures/common/dirtymirror1a
{
    //dirt pass
    deformVertexes wave 300 sin 0 1 0 0.5
    qer_editorimage textures/common/dirtymirror.tga
    surfaceparm slag    
    portal

    {
		map textures/common/mirror1a.tga
		blendFunc GL_ONE_MINUS_SRC_ALPHA GL_SRC_ALPHA
		depthWrite
    }
    {
		map textures/common/dirtymirrora.tga
		blendFunc GL_ONE_MINUS_SRC_ALPHA GL_SRC_ALPHA
		tcMod stretch sin 1 0.02 0 .05
    }
}

