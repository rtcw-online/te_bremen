{
	q3map_patchshadows
	surfaceparm metalsteps
	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/swf/spipe_02.tga
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

