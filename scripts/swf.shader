textures/swf/fuse_box_ad
{
	polygonOffset
	entityMergable
	{
		map textures/swf/fuse_box_ad.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		tcmod rotate 120
		rgbGen vertex
		alphaGen vertex
	}
}

